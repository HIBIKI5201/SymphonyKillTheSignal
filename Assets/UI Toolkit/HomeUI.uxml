<engine:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:engine="UnityEngine.UIElements" xmlns:editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI%20Toolkit/StyleSheet/MainUI.uss?fileID=7433441132597879392&amp;guid=030672bf8791f77439b2e02cd045061b&amp;type=3#MainUI" />
    <Style src="project://database/Assets/UI%20Toolkit/StyleSheet/TitleUI.uss?fileID=7433441132597879392&amp;guid=2fd435c00491bc34cbb5e65e94baed3d&amp;type=3#TitleUI" />
    <Style src="project://database/Assets/UI%20Toolkit/StyleSheet/HomeUI.uss?fileID=7433441132597879392&amp;guid=6ce4a178eea1e034e9170cd740f3f5cf&amp;type=3#HomeUI" />
    <engine:VisualElement name="StatusParameter" style="width: 80%; height: 10%; top: 0; left: 0;" />
    <engine:VisualElement style="width: 85%; height: 30%; left: auto; top: auto; right: auto; position: absolute; bottom: 40px; align-items: center; align-self: center; justify-content: flex-end; flex-direction: row-reverse; margin-top: 0; margin-right: 0; margin-bottom: 0; margin-left: 0; text-shadow: 2px 2px 10px rgba(0, 0, 0, 0.75);">
        <engine:Button text="進む" name="MovementButton" class="homeButton button" />
        <engine:Button text="収集する" name="CollectButton" class="homeButton button" />
        <engine:Button text="キャンプ" name="CampButton" class="homeButton button" />
        <engine:Button text="インベントリ" name="ItemButton" class="homeButton button" />
    </engine:VisualElement>
    <engine:VisualElement name="CampWindow" class="window" style="display: flex; visibility: hidden; padding-left: 0; padding-top: 0; padding-right: 0; padding-bottom: 0; -unity-font-definition: url(&quot;project://database/Assets/font/JapaneseFont.ttf?fileID=12800000&amp;guid=bf12a9e7c0cbc8e428480f2d8f446958&amp;type=3#JapaneseFont&quot;);">
        <engine:VisualElement style="width: 100%; height: 20%; flex-grow: 1; flex-direction: row; font-size: 50px; color: rgb(255, 255, 255); -unity-text-align: middle-center; text-shadow: 2px 2px 10px rgb(0, 0, 0);">
            <engine:VisualElement name="Camp-Bonfire" class="camp-button-active">
                <engine:Label text="焚火" />
            </engine:VisualElement>
            <engine:VisualElement name="Camp-Rest" class="camp-button-inactive">
                <engine:Label text="休息" />
            </engine:VisualElement>
            <engine:VisualElement name="Camp-Repair" class="camp-button-inactive">
                <engine:Label text="修理" />
            </engine:VisualElement>
        </engine:VisualElement>
        <engine:VisualElement style="height: 100%; width: 100%; flex-direction: row; visibility: visible; background-color: rgba(51, 51, 51, 0.75); border-left-color: rgb(64, 64, 64); border-right-color: rgb(64, 64, 64); border-top-color: rgb(64, 64, 64); border-bottom-color: rgb(64, 64, 64); border-bottom-width: 3px; border-left-width: 3px; border-right-width: 3px;">
            <engine:VisualElement name="Camp-BonfireWindow" class="window-element" style="width: 100%; height: 100%; position: absolute; justify-content: space-evenly;">
                <engine:SliderInt label="枝を投入する" value="1" high-value="10" low-value="1" name="Bonfire-Slider" direction="Horizontal" style="width: 80%; height: 20%; font-size: 75%; -unity-font-definition: url(&quot;project://database/Assets/font/JapaneseFont.ttf?fileID=12800000&amp;guid=bf12a9e7c0cbc8e428480f2d8f446958&amp;type=3#JapaneseFont&quot;); -unity-text-align: middle-center; color: rgb(255, 255, 255);" />
                <engine:VisualElement style="visibility: hidden; width: 100%; flex-direction: row; height: 40%;">
                    <engine:VisualElement style="flex-grow: 1; width: 100%; -unity-text-align: middle-center; visibility: visible; font-size: 55px; flex-direction: row;">
                        <engine:VisualElement style="justify-content: space-around; -unity-text-align: middle-right; background-image: none;">
                            <engine:Label text="消費する枝数：" class="textBox" style="text-overflow: ellipsis; color: rgb(255, 255, 255);" />
                            <engine:Label text="消費時間：" class="textBox" style="text-overflow: ellipsis; color: rgb(255, 255, 255);" />
                        </engine:VisualElement>
                        <engine:VisualElement style="flex-grow: 1; justify-content: space-around; -unity-text-align: middle-left;">
                            <engine:Label text="5本" class="textBox" style="text-overflow: ellipsis; color: rgb(255, 255, 255);" />
                            <engine:Label text="1時間" class="textBox" style="text-overflow: ellipsis; color: rgb(255, 255, 255);" />
                        </engine:VisualElement>
                    </engine:VisualElement>
                    <engine:VisualElement style="flex-grow: 1; width: 50%; padding-top: 3%; padding-right: 3%; padding-bottom: 3%; padding-left: 3%; justify-content: center; align-items: center;">
                        <engine:VisualElement name="Bonfire-Image" style="visibility: visible; background-image: url(&quot;project://database/Assets/Sprite/Square.png?fileID=7482667652216324306&amp;guid=9e91330c14ea62c4da62473701014cdc&amp;type=3#Square&quot;); width: 100%; height: 100%;" />
                    </engine:VisualElement>
                </engine:VisualElement>
                <engine:Button text="焚火を作る" name="Rest-Button" class="homeButton button" style="height: 18%; margin-top: 0; margin-right: 0; margin-bottom: 0; margin-left: 0; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; width: 100%; flex-grow: 0; -unity-font-style: bold;" />
            </engine:VisualElement>
            <engine:VisualElement name="Camp-RestWindow" class="window-element" style="width: 100%; height: 100%; position: absolute;" />
            <engine:VisualElement name="Camp-RepairWindow" class="window-element" style="width: 100%; height: 100%; position: absolute;" />
        </engine:VisualElement>
    </engine:VisualElement>
    <engine:VisualElement name="MovementWindow" class="window" style="display: none; justify-content: space-evenly;">
        <engine:SliderInt label="歩く時間" value="1" high-value="10" low-value="1" name="Movement-Slider" direction="Horizontal" style="width: 80%; height: 20%; font-size: 75%; -unity-font-definition: url(&quot;project://database/Assets/font/JapaneseFont.ttf?fileID=12800000&amp;guid=bf12a9e7c0cbc8e428480f2d8f446958&amp;type=3#JapaneseFont&quot;); -unity-text-align: middle-center; color: rgb(255, 255, 255);" />
        <engine:VisualElement style="align-items: center; justify-content: flex-start; height: auto;">
            <engine:VisualElement name="VisualElement" style="height: auto; width: 100%; flex-direction: row; align-self: center; align-items: center; justify-content: center; -unity-font-definition: url(&quot;project://database/Assets/font/JapaneseFont.ttf?fileID=12800000&amp;guid=bf12a9e7c0cbc8e428480f2d8f446958&amp;type=3#JapaneseFont&quot;); font-size: 75px; -unity-font-style: bold; color: rgb(255, 255, 255); margin-bottom: 35px;">
                <engine:Label text="推定移動距離：" style="width: auto; -unity-text-align: upper-right;" />
                <engine:VisualElement style="width: 30%;">
                    <engine:Label text="100km" name="Movement-DistanceText" />
                </engine:VisualElement>
            </engine:VisualElement>
            <engine:VisualElement style="height: auto; width: 100%; flex-direction: row; align-self: center; align-items: center; justify-content: center; -unity-font-definition: url(&quot;project://database/Assets/font/JapaneseFont.ttf?fileID=12800000&amp;guid=bf12a9e7c0cbc8e428480f2d8f446958&amp;type=3#JapaneseFont&quot;); font-size: 60px; -unity-font-style: bold; color: rgb(255, 255, 255);">
                <engine:Label text="消費時間：" style="width: 30%; -unity-text-align: upper-right;" />
                <engine:VisualElement style="width: 30%;">
                    <engine:Label text="1時間" name="Movement-TimeText" style="-unity-text-align: upper-left;" />
                </engine:VisualElement>
            </engine:VisualElement>
            <engine:VisualElement style="height: auto; width: 100%; flex-direction: row; align-self: center; align-items: center; justify-content: center; -unity-font-definition: url(&quot;project://database/Assets/font/JapaneseFont.ttf?fileID=12800000&amp;guid=bf12a9e7c0cbc8e428480f2d8f446958&amp;type=3#JapaneseFont&quot;); font-size: 60px; -unity-font-style: bold; color: rgb(255, 255, 255);">
                <engine:Label text="消費体力：" style="width: 30%; -unity-text-align: upper-right;" />
                <engine:VisualElement name="VisualElement" style="width: 30%;">
                    <engine:Label text="1.0" name="Movement-HealthText" style="-unity-text-align: upper-left;" />
                </engine:VisualElement>
            </engine:VisualElement>
        </engine:VisualElement>
        <engine:Button text="移動開始" name="Movement-Button" class="homeButton button" style="height: 18%; margin-top: 0; margin-right: 0; margin-bottom: 0; margin-left: 0; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; width: 100%; flex-grow: 0; -unity-font-style: bold;" />
    </engine:VisualElement>
</engine:UXML>
